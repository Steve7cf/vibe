{
  "name": "vibe",
  "version": "1.0.0",
  "description": "Vibe - Professional Desktop Music Player",
  "homepage": "https://github.com/Steve7cf/vibe",
  "main": "src/main/main.js",
  "scripts": {
    "start": "GDK_BACKEND=x11 ELECTRON_DISABLE_SECURITY_WARNINGS=1 electron .",
    "dev": "GDK_BACKEND=x11 ELECTRON_DISABLE_SECURITY_WARNINGS=1 electron . --dev",
    "build": "electron-builder --linux",
    "pack": "electron-builder --dir"
  },
  "build": {
    "appId": "com.vibe.musicplayer",
    "productName": "Vibe",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Audio",
      "maintainer": "Steve Bazaar <stevebazaar99@gmail.com>"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "music-metadata": "^10.0.0",
    "node-id3": "^0.2.6"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0"
  },
  "keywords": [
    "music",
    "player",
    "electron"
  ],
  "author": {
    "name": "Steve Bazaar",
    "email": "stevebazaar99@gmail.com"
  },
  "license": "MIT"
}
